{
  "name": "gdsc-uos-design-system",
  "version": "0.1.0",
  "description": "React components library used in gdsc-uos",
  "author": "LeeSangMin",
  "packageManager": "yarn@3.2.1",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/poiu694/gdsc-uos-recruit-page",
    "directory": "packages/design-system"
  },
  "type": "module",
  "files": [
    "dist",
    "esm"
  ],
  "main": "./src/index.ts",
  "types": "./src/index.d.ts",
  "scripts": {
    "clean:build": "rm -rf dist && rm -rf esm",
    "build": "yarn clean:build && cross-env BABEL_ENV=build rollup -c && tsc --declaration --emitDeclarationOnly --declarationDir dist",
    "test": "cross-env BABEL_ENV=test",
    "prepack": "yarn && yarn build",
    "prettier": "prettier --write ./src",
    "lint": "eslint --ext **/*.ts **/*.tsx"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "emotion-reset": "^3.0.1",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.7",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gdsc-uos-eslint-config-custom": "workspace:^",
    "prettier": "^2.8.4",
    "rollup": "^3.9.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslib": "^2.4.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.3",
    "emotion-reset": "^3.0.1",
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "exports": {
    ".": {
      "require": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "require": {
          "default": "./dist/index.js",
          "types": "./dist/index.d.ts"
        },
        "import": {
          "default": "./esm/src/index.js",
          "types": "./dist/index.d.ts"
        }
      },
      "./package.json": "./package.json"
    },
    "types": "./dist/index.d.ts"
  }
}
